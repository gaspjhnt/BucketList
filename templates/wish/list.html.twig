{% extends 'base.html.twig' %}

{% block body %}
    <h1>List of whises</h1>
    <section class="wish-list">
        {% for wish in wishes %}
            <a href="{{ path('wish_show', {'id': wish.id}) }}">
               <article class="card">
                   <h4>{{ wish.title | nl2br}} </h4>
               </article>
            </a>

        {% endfor %}
    </section>



{% endblock %}

{% block title %}
    {{ parent() }} | LIST
{% endblock %}

